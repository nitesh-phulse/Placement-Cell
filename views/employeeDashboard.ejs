<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List Of All Students</title>
    <!-- Include the external stylesheet for styling -->
    <link rel="stylesheet" href="/css/employeeDashboard.css">
    
</head>
<body>
    <!-- Navigation bar -->
    <nav>
        <a class="logo">CampusPro</a>
        <a href="#" class="home">Home</a>
        <a href="/employee/student">Student</a>
        <a href="/student/interview_list">Interview List</a>
        <a href="/job/list">Placement Cell</a>
        <a href="/employee/download">Download Data</a>
        <a href="/destroy_session" class="signout">Sign Out</i></a>
    </nav>
     <!-- Main content section -->
    <section>
        <div>
              <!-- Page title -->
            <h1>List Of All Student </h1>
            <!-- Table to display student information -->
            <table>
                <!-- Table headers -->
                <tr>
                    <th>Serial No.</th>
                    <th>Batch</th>
                    <th>Student Name</th>
                    <th>Email</th>
                    <th>College</th>
                    <th>Status</th>
                    <th>DSA Final Score</th>
                    <th>WebD Final Score</th>
                    <th>React Final Score</th>
                    <th>Action</th>
                </tr>
                <!-- Loop through studentList data and generate table rows -->
                <% for (var i = 0; i < studentList.length; i++) { %>
                <tr>
                    <td><%= i + 1 %></td>
                    <td><%= studentList[i].batch %></td>
                    <td><%= studentList[i].name %></td>
                    <td><%= studentList[i].email %></td>
                    <td><%= studentList[i].college %></td>
                    <td><%= studentList[i].status %></td>
                    <td><%= studentList[i].DSA_FinalScore %></td>
                    <td><%= studentList[i].WebD_FinalScore %></td>
                    <td><%= studentList[i].React_FinalScore %></td>
                    <td>
                        <!-- Button to allocate an interview for the student -->
                        <button>
                            <a id="linkToAllocateInterview" href="/student/<%= studentList[i].id %>">Interview Allocate</a>
                        </button>
                    </td>
                </tr>
                <% } %>
            </table>
        </div>
    </section>
</body>
</html>
